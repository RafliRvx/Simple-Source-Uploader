{
  "version": 2,
  "builds": [
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/check.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/check",
      "dest": "/api/check.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/check.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
